<launch>
  <node name="flashgo_node"          pkg="flashgo"  type="flashgo_node" output="screen">
    <param name="serial_port"         type="string" value="/dev/ttyUSB0"/>  
    <param name="serial_baudrate"     type="int"    value="230400"/>
    <param name="frame_id"            type="string" value="laser"/>
    <param name="inverted"            type="bool"   value="false"/>
    <param name="angle_compensate"    type="bool"   value="true"/>
    <param name="ignore_array"   type="string" value="10,50,80,200" />
    <param name="ignore_value"  type="double"  value="100" />
  </node>
</launch>
